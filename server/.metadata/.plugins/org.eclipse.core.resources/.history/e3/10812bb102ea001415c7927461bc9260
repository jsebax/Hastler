package business;

import java.util.List;

import entity.Servi;
import repositories.ServiRepository;

public class GetAllService implements UnitOfWork {

	ServiRepository serviMongo;
	List<Servi> persons;
	
	public List<Servi> getPersons() {
		return persons;
	}

	public void setPersons(List<Servi> persons) {
		this.persons = persons;
	}

	public ServiRepository getServiMongo() {
		return serviMongo;
	}

	public void setServiMongo(ServiRepository serviMongo) {
		this.serviMongo = serviMongo;
	}

	public GetAllService() {
		super();
	}

	@Override
	public boolean run() {
		persons = serviMongo.findAllServi();
		return false;
	}

}
